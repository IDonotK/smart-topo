{
  "name": "rocketbot",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "docker:run": "docker build -t rocketbot . && docker run --name rocketbot -p 8888:80 -d -e SKYWALKING_URL=localhost:1234,127.0.0.1:1235 rocketbot",
    "docker:stop": "docker stop rocketbot && docker rm rocketbot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "d3": "^5.9.2",
    "d3-tip": "^0.9.1",
    "dayjs": "^1.8.8",
    "echarts": "^4.9.0",
    "element-ui": "^2.14.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "noty": "^3.2.0-beta",
    "overlayscrollbars": "^1.13.0",
    "overlayscrollbars-vue": "^0.2.2",
    "popper.js": "^1.14.7",
    "vue": "^2.6.11",
    "vue-class-component": "^6.0.0",
    "vue-i18n": "^8.10.0",
    "vue-js-modal": "^1.3.31",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.0.1",
    "vue-virtual-scroller": "^1.0.10",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.4",
    "@types/lodash": "^4.14.138",
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-typescript": "^3.4.1",
    "@vue/cli-service": "^3.4.1",
    "babel-plugin-component": "^1.1.1",
    "husky": "^4.0.9",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "pug": "^2.0.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.2.0",
    "svg-sprite-loader": "^4.1.3",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.6.11"
  }
}
