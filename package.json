{
  "name": "rocketbot",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "npm run eslint && npm run prettier && npm run stylelint",
    "eslint": "./node_modules/.bin/eslint --ext .js,.ts,.vue src --fix",
    "prettier": "./node_modules/.bin/prettier --write src/**/*.{js,ts,vue,css,sass,scss}",
    "stylelint": "./node_modules/.bin/stylelint src/**/*.{vue,css,sass,scss} --fix",
    "docker:run": "docker build -t rocketbot . && docker run --name rocketbot -p 8888:80 -d -e SKYWALKING_URL=localhost:1234,127.0.0.1:1235 rocketbot",
    "docker:stop": "docker stop rocketbot && docker rm rocketbot"
  },
  "lint-staged": {
    "*.{ts,vue,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "acorn": "^6.0.0",
    "axios": "^0.18.0",
    "d3": "^5.9.2",
    "d3-tip": "^0.9.1",
    "dayjs": "^1.8.8",
    "echarts": "^4.9.0",
    "element-ui": "^2.14.0",
    "github-markdown-css": "^4.0.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.1",
    "markdown-it-table-of-contents": "^0.5.2",
    "noty": "^3.2.0-beta",
    "overlayscrollbars": "^1.13.0",
    "overlayscrollbars-vue": "^0.2.2",
    "popper.js": "^1.14.7",
    "vue": "^2.6.11",
    "vue-class-component": "^6.0.0",
    "vue-i18n": "^8.10.0",
    "vue-js-modal": "^1.3.31",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.0.1",
    "vue-virtual-scroller": "^1.0.10",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1",
    "vxe-table": "^3.0.13",
    "xe-utils": "^3.0.4"
  },
  "devDependencies": {
    "@types/d3": "^6.2.0",
    "@types/d3-tip": "^3.5.5",
    "@types/jquery": "^3.5.4",
    "@types/lodash": "^4.14.138",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-typescript": "^3.4.1",
    "@vue/cli-service": "^3.4.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "^4.0.9",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "pug": "^2.0.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^13.10.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.2.0",
    "svg-sprite-loader": "^4.1.3",
    "text-loader": "0.0.1",
    "typescript": "^3.9.9",
    "vue-eslint-parser": "^7.1.0",
    "vue-template-compiler": "^2.6.11"
  }
}
